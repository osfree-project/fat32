     $(PROJ_BLD)\qfloat.$(OBJEXT) &
     $(PROJ_BLD)\qint.$(OBJEXT) &
     $(PROJ_BLD)\qlist.$(OBJEXT) &
     $(PROJ_BLD)\qstring.$(OBJEXT) &
